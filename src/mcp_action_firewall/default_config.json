    "allow_prefixes": [
      "get_",
      "list_",
      "read_",
      "fetch_"
    ],
    "block_keywords": [
      "delete",
      "update",
      "create",
      "pay",
      "send",
      "transfer",
      "drop",
      "remove",
      "refund"
    ],
    "default_action": "block",
